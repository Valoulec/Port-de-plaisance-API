{
  "source": {
    "include": ["controllers", "routes", "middleware", "models", "app.js"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js$"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
